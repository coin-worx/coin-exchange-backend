<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="CoinExchange.Trades.ReadModel" namespace="CoinExchange.Trades.ReadModel.DTO">
  <class name="OrderReadModel" table="CoinExchange.order" lazy="false">
    <id name="OrderId" column="OrderId">
      <generator class="assigned" />
    </id>
    <property name="Side"  column="OrderSide"/>
    <property name="Type"  column="OrderType"/>
    <property name="DateTime"  column="OrderDateTime"/>
    <property name="Price"  column="Price"/>
    <property name="VolumeExecuted"  column="VolumeExecuted"/>
    <property name="Volume"  column="Volume"/>
    <property name="OpenQuantity"  column="OpenQuantity"/>
    <property name="Status"  column="Status"/>
    <property name="TraderId"  column="TraderId"/>
    <property name="CurrencyPair"  column="CurrencyPair"/>
    <property name="ClosingDateTime"  column="ClosingDateTime"/>
    <property name="AveragePrice"  column="AveragePrice"/>
    </class>
</hibernate-mapping>